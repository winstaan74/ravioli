                <ucd>REDUCT_PARAM</ucd>
                <dataType>double</dataType>
            </column>
            <column>
                <name>skylevel</name>
                <description>Sky level</description>
                <unit>count</unit>
                <ucd>OBS_PARAM</ucd>
                <dataType>double</dataType>
            </column>
            <column>
                <name>skynoise</name>
