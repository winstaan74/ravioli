        <table xmlns:tab="urn:astrogrid:schema:TableMetadata">
            <name>ChipsAll</name>
            <description>This table contains the CCD information. Each observation here records the CCD information, also referred to as the Data Quality Control in formation since it provides the seeing, ellipticity, magnitude zero point, mangnitude limit, exposure time, astrometry, etc.</description>
            <column>
                <name>chipID</name>
                <description>chip ID</description>
                <ucd>ID_MAIN</ucd>
                <dataType>long</dataType>
            </column>
            <column>
                <name>runno</name>
                <description>Run number</description>
                <ucd>OBS_RUN</ucd>
                <dataType>int</dataType>
            </column>
            <column>
                <name>ra</name>
                <description>R.A. of centre of chip</description>
                <unit>deg</unit>
                <ucd>POS_EQ_RA_MAIN</ucd>
                <dataType>double</dataType>
            </column>
            <column>
                <name>dec</name>
                <description>Declination of centre of chip</description>
                <unit>deg</unit>
                <ucd>POS_EQ_DEC_MAIN</ucd>
                <dataType>double</dataType>
            </column>
            <column>
                <name>naxis1</name>
                <description>Dimension of CCD across first axis</description>
                <unit>pixel</unit>
                <ucd>INST_DET_SIZE</ucd>
                <dataType>int</dataType>
            </column>
            <column>
                <name>naxis2</name>
                <description>Dimension of CCD across second axis</description>
                <unit>pixel</unit>
                <ucd>INST_DET_SIZE</ucd>
                <dataType>int</dataType>
            </column>
            <column>
                <name>chipno</name>
                <description>CCD number</description>
                <dataType>short</dataType>
            </column>
            <column>
                <name>mjd</name>
                <description>MJD of observation</description>
                <unit>day</unit>
                <ucd>TIME_DATE</ucd>
                <dataType>double</dataType>
            </column>
            <column>
                <name>magzpt</name>
                <description>Magnitude zero point</description>
                <unit>mag</unit>
                <ucd>PHOT_MAG_ZERO</ucd>
                <dataType>double</dataType>
            </column>
            <column>
                <name>magzrr</name>
                <description>Magnitude zero point error</description>
                <unit>mag</unit>
                <ucd>ERROR</ucd>
                <dataType>double</dataType>
            </column>
            <column>
                <name>extinct</name>
                <description>Extinction</description>
                <unit>mag</unit>
                <ucd>PHOT_EXTINCTION_TOTAL</ucd>
                <dataType>double</dataType>
            </column>
            <column>
                <name>airmass</name>
                <description>Airmass</description>
                <ucd>PHOT_ATM_AIRMASS</ucd>
                <dataType>double</dataType>
            </column>
            <column>
                <name>exptime</name>
                <description>Exposure time</description>
                <unit>second</unit>
                <ucd>TIME_EXPTIME</ucd>
                <dataType>double</dataType>
            </column>
            <column>
                <name>night</name>
                <description>Night of observation</description>
                <ucd>TIME_DATE</ucd>
                <dataType>int</dataType>
            </column>
            <column>
                <name>dateobs</name>
                <description>Date of observation</description>
                <ucd>TIME_DATE</ucd>
                <dataType>int</dataType>
            </column>
            <column>
                <name>band</name>
